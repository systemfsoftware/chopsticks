{
	"name": "@systemfsoftware/chopsticks-core",
	"version": "0.14.2-1-beta.1",
	"author": "Acala Developers <hello@acala.network>",
	"license": "Apache-2.0",
	"type": "module",
	"scripts": {
		"clean": "rm -rf dist tsconfig.tsbuildinfo",
		"build": "yarn clean && ../../package-build.sh",
		"docs:prep": "typedoc",
		"depcheck": "npx depcheck"
	},
	"dependencies": {
		"@polkadot/rpc-provider": "^12.3.1",
		"@polkadot/types": "^12.3.1",
		"@polkadot/types-codec": "^12.3.1",
		"@polkadot/types-known": "^12.3.1",
		"@polkadot/util": "^13.0.2",
		"@polkadot/util-crypto": "^13.0.2",
		"@systemfsoftware/chopsticks-executor": "workspace:*",
		"comlink": "^4.4.1",
		"eventemitter3": "^5.0.1",
		"lodash": "^4.17.21",
		"lru-cache": "^10.2.0",
		"pino": "^8.19.0",
		"pino-pretty": "^11.0.0",
		"rxjs": "^7.8.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@swc/cli": "0.4.0",
		"@swc/core": "^1.7.6",
		"@types/lodash": "^4.17.6",
		"typescript": "^5.5.3",
		"vitest": "^1.4.0"
	},
	"files": [
		"dist/esm/**",
		"dist/cjs/**"
	],
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js",
			"default": "./dist/esm/index.js"
		},
		"./*": {
			"require": "./dist/cjs/*.js",
			"import": "./dist/esm/*.js",
			"default": "./dist/esm/*.js"
		}
	},
	"browser": {
		"./dist/cjs/wasm-executor/node-worker.js": "./dist/cjs/wasm-executor/browser-worker.js",
		"./dist/esm/wasm-executor/node-worker.js": "./dist/esm/wasm-executor/browser-worker.js"
	}
}
